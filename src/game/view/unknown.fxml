<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<GridPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:id="utoken"
            style="-fx-padding: 20px"
            vgap="10" hgap="10">

    <!-- 1행 -->
    <ImageView fitWidth="50" fitHeight="50" GridPane.columnIndex="0" GridPane.rowIndex="0">
        <Image url="@../res/b`.png" />
    </ImageView>
    <ImageView fitWidth="50" fitHeight="50" GridPane.columnIndex="1" GridPane.rowIndex="0">
        <Image url="@../res/g`.png" />
    </ImageView>

    <!-- 2행 -->
    <ImageView fitWidth="50" fitHeight="50" GridPane.columnIndex="0" GridPane.rowIndex="1">
        <Image url="@../res/p`.png" />
    </ImageView>
    <ImageView fitWidth="50" fitHeight="50" GridPane.columnIndex="1" GridPane.rowIndex="1">
        <Image url="@../res/R`.png" />
    </ImageView>

    <!-- 3행 -->
    <ImageView fitWidth="50" fitHeight="50" GridPane.columnIndex="0" GridPane.rowIndex="2" GridPane.columnSpan="2">
        <Image url="@../res/y`.png" />
    </ImageView>
</GridPane>
